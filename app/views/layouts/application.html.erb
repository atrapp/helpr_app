<!DOCTYPE html>
<html>
<head>
  <title>MyHelprApp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
    <div class="wrapper">

      <div>
        <h1>
          <%= flash.now[:alert] %>
        </h1>
        <h1>
          <%= flash.now[:notice] %>
        </h1>
      </div>

      <%= yield %>

    </div>
</body>
</html>
