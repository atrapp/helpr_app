<%= form_for @request do |f| %>

    <%= f.label :title %>
    <%= f.text_field :title %><br />            
    
    <%= f.label :description %>
    <%= f.text_field :description %><br />
          
    <%= f.label :date_start %>
    <%= f.text_field :date_start %><br />
  
    <%= f.label :date_end %>
    <%= f.text_field :date_end %><br />
    
    <%= f.label :user %>
    <%= f.text_field :user %><br />
    
    <%= f.label :category %>
    <%= f.text_field :category %><br />
   
    <%= f.label :location %>
    <%= f.text_field :location %><br />
        
    <%= f.submit %>       

<% end %>

