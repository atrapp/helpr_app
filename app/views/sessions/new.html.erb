<h2>Log-In Form</h2>

<div class="container">

    <%= form_tag sessions_path do %>
      <%= label_tag :email %>
      <%= text_field_tag :email, "user@test.com" %>
      <%= label_tag :password %>
      <%= password_field_tag :password, "password" %>
      <button>Log in</button>
    <% end %>

</div>