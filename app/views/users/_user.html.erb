<%= form_for @user do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name %><br />            
    
    <%= f.label :address %>
    <%= f.text_field :address %><br />
  
    <%= f.label :gender %>
    <%= radio_button("user", "gender", "female") %>female
    <%= radio_button("user", "gender", "male") %>male<br />
    
    <%= f.label :age %>
    <%= f.text_field :age %><br />
  
    <%= f.label :phone %>
    <%= f.text_field :phone %><br />
    
    <%= f.label :email %>
    <%= f.text_field :email %><br />
    
    <%= f.label :description %>
    <%= f.text_field :description %><br />
   
    <%= f.label :rating %>
    <%= f.text_field :rating %><br />
   
    <%= f.label :picUrl %>
    <%= f.text_field :picUrl %><br />
     
    <%= f.submit %>       

<% end %>

